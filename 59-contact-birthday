public Boolean isBirthday(Contact c) {
    //code here
    Integer month = c.Birthdate.month();
    Integer day = c.Birthdate.day();

    Date date1 = System.today();
    Integer day1 = date1.day();
    Integer month1 = date1.month();


    if(day==day1 && month==month1){
        return true;
    }
    return false;
}