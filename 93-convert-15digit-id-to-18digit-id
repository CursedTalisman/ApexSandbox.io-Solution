public String convert15to18DigitId(String fifteenDigit)
{
    //code here
    if(fifteenDigit==NULL)
        return NULL;
    else if(fifteenDigit.length()!=15){
        return '-1';
    }else{
        Id eightteenDigit = fifteenDigit;
        String str = Id.valueOf('eightteenDigit');
        return str;
    }
}